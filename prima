#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
    int awal;
    int akhir;
    int i;
    int prima;

    cout << "Awal   :   " << endl;
    cin >> awal;
    cout << "Akhir  :   " << endl;
    cin >> akhir;
    cout << endl;

    for (awal=awal; awal<=akhir; awal++){
        prima=0;
        for (i=2; i<awal; i++){

    if (awal % i == 0){
        prima = 1;
}
}
    if (prima == 0){
        cout << awal++<<", ";
}
}
    getch();
    return 0;
}
